{
	"name": "tapris",
	"version": "3.0.0",
	"main": "dist/index.js",
	"scripts": {
		"start": "node .",
		"build": "tsc",
		"prod": "tsc && node .",
		"build:watch": "tsc -w",
		"format": "npx prettier --write ./src",
		"lint": "eslint ./src --ext .ts",
		"lint:fix": "eslint ./src --ext .ts --fix",
		"pm2:start": "pm2 start . --name 'tapris'",
		"pm2:start:watch": "pm2 start . --name 'tapris' --watch",
		"pm2:stop": "pm2 stop ."
	},
	"author": {
		"email": "alexiy.rybin@gmail.com",
		"name": "Alexiy Rybin",
		"url": "https://rybin.vercel.app/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tapnisu/tapris.git"
	},
	"bugs": {
		"url": "https://github.com/tapnisu/tapris/issues"
	},
	"keywords": [
		"bot",
		"discord"
	],
	"license": "MIT",
	"dependencies": {
		"@discordjs/opus": "^0.8.0",
		"@discordjs/voice": "^0.11.0",
		"@iamtraction/google-translate": "^1.1.2",
		"avconv": "^3.1.0",
		"axios": "^0.27.2",
		"bufferutil": "^4.0.6",
		"canvas": "^2.9.3",
		"discord.js": "^14.1.2",
		"dotenv": "^16.0.1",
		"erlpack": "^0.1.4",
		"ffmpeg": "^0.0.4",
		"ffmpeg-static": "^5.0.2",
		"genshin-db": "^4.2.3",
		"html-to-text": "^8.2.1",
		"sodium": "^3.0.2",
		"typeface-jetbrains-mono": "^2.0.1",
		"utf-8-validate": "^5.0.9",
