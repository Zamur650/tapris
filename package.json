{
	"name": "tapris",
	"version": "3.2.1",
	"main": "dist/index.js",
	"scripts": {
		"start": "node .",
		"build": "tsc",
		"build:watch": "tsc -w",
		"prod": "tsc && node .",
		"test": "jest",
		"cmdsExport": "node ./dist/cmdsExport.js",
		"format": "npx prettier --write ./src && eslint ./src --ext .ts --fix",
		"lint": "eslint ./src --ext .ts",
		"lint:fix": "eslint ./src --ext .ts --fix",
		"pm2:start": "pm2 start . --name 'tapris' --exp-backoff-restart-delay=100",
		"pm2:start:watch": "pm2 start . --name 'tapris' --watch --exp-backoff-restart-delay=100",
		"pm2:stop": "pm2 stop .",
		"prisma:migrate": "npx prisma migrate dev --name init",
		"prisma:generate": "npx prisma generate"
	},
	"author": {
		"email": "alexiy.rybin@gmail.com",
		"name": "Alexiy Rybin",
		"url": "https://tapni.su/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tapris-bot/tapris-node.git"
	},
	"bugs": {
		"url": "https://github.com/tapris-bot/tapris-node/issues"
	},
	"keywords": [
		"bot",
		"discord"
	],
	"license": "MIT",
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@discordjs/voice": "^0.14.0",
		"@iamtraction/google-translate": "^2.0.1",
		"@napi-rs/canvas": "^0.1.34",
		"@prisma/client": "^4.10.1",
		"avconv": "^3.1.0",
		"axios": "^1.3.3",
		"discord.js": "^14.7.1",
		"dotenv": "^16.0.3",
		"ffmpeg": "^0.0.4",
		"ffmpeg-static": "^5.1.0",
		"genshin-db": "^4.3.5",
		"html-to-text": "^9.0.4",
		"sodium-native": "^4.0.1",
		"typeface-jetbrains-mono": "^2.0.1",
		"waifu.js": "^1.0.0",
		"youtube-sr": "^4.3.4",
		"ytdl-core": "^4.11.2"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.7",
		"erlpack": "^0.1.4",
		"utf-8-validate": "^6.0.2",
		"zlib-sync": "^0.1.8"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.18.6",
		"@types/jest": "^29.4.0",
		"@types/node": "^18.13.0",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"babel-jest": "^29.4.2",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.4.2",
		"prettier": "^2.8.4",
		"prisma": "^4.10.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5"
	},
	"peerDependencies": {
		"pm2": ">=5"
	},
	"peerDependenciesMeta": {
		"pm2": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=16"
	}
}
