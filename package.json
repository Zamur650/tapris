{
  "name": "tapris",
  "description": "Multipurpose discord bot",
  "version": "3.3.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prod": "tsc && node .",
    "test": "node --test",
    "commands:export": "node ./dist/commandsExport.js",
    "commands:delete": "node ./dist/commandsDelete.js",
    "format": "npx prettier . --check",
    "format:fix": "npx prettier . --write",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "pm2:start": "pm2 start . --name 'tapris' --exp-backoff-restart-delay=100",
    "pm2:start:watch": "pm2 start . --name 'tapris' --watch --exp-backoff-restart-delay=100",
    "pm2:stop": "pm2 stop .",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate"
  },
  "author": {
    "email": "0xalekseirybin@gmail.com",
    "name": "Aleksei Rybin",
    "url": "https://tapni.su/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapris-bot/tapris-node.git"
  },
  "bugs": {
    "url": "https://github.com/tapris-bot/tapris-node/issues"
  },
  "keywords": [
    "bot",
    "discord"
  ],
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.1",
    "@iamtraction/google-translate": "^2.0.1",
    "@napi-rs/canvas": "^0.1.51",
    "@prisma/client": "^5.11.0",
    "avconv": "^3.1.0",
    "axios": "^1.6.8",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^5.2.0",
    "genshin-db": "^5.1.2",
    "html-to-text": "^9.0.5",
    "sodium-native": "^4.0.10",
    "typeface-jetbrains-mono": "^2.0.1",
    "waifu.js": "^1.0.0",
    "youtube-sr": "^4.3.10",
    "ytdl-core": "^4.11.5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "erlpack": "^0.1.4",
    "utf-8-validate": "^6.0.3",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@types/node": "^20.11.29",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "pm2": ">=5.3.1"
  },
  "peerDependenciesMeta": {
    "pm2": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16"
  },
  "imports": {
    "#commands/*": "./dist/commands/*",
    "#components/*": "./dist/components/*",
    "#core/*": "./dist/core/*",
    "#events/*": "./dist/events/*",
    "#interfaces/*": "./dist/interfaces/*",
    "#lib/*": "./dist/lib/*",
    "#locales/*": "./dist/locales/*",
    "#db/*": "./dist/db/*"
  },
  "type": "module"
}
