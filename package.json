{
  "name": "tapris",
  "description": "Multipurpose discord bot",
  "version": "3.4.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "start:watch": "node . --watch",
    "start:migrate:prod": "prisma migrate deploy && node .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prod": "tsc && node .",
    "test": "node --test",
    "commands:export": "node ./dist/commandsExport.js",
    "commands:delete": "node ./dist/commandsDelete.js",
    "format": "npx prettier . --check",
    "format:fix": "npx prettier . --write",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix"
  },
  "author": {
    "email": "0xalekseirybin@gmail.com",
    "name": "Aleksei Rybin",
    "url": "https://tapni.su/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapris-bot/tapris-node.git"
  },
  "bugs": {
    "url": "https://github.com/tapris-bot/tapris-node/issues"
  },
  "keywords": [
    "bot",
    "discord"
  ],
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.17.0",
    "@iamtraction/google-translate": "^2.0.1",
    "@napi-rs/canvas": "^0.1.53",
    "@prisma/client": "^5.14.0",
    "avconv": "^3.1.0",
    "axios": "^1.7.2",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "ffmpeg": "^0.0.4",
    "genshin-db": "^5.1.5",
    "html-to-text": "^9.0.5",
    "i18n": "^0.15.1",
    "sodium-native": "^4.1.1",
    "typeface-jetbrains-mono": "^2.0.1",
    "waifu.js": "^1.0.0",
    "youtube-sr": "^4.3.11",
    "ytdl-core": "^4.11.5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "erlpack": "^0.1.4",
    "ffmpeg-static": "^5.2.0",
    "utf-8-validate": "^6.0.4",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "node-gyp": "^10.1.0",
    "prettier": "^3.3.2",
    "prisma": "^5.15.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "imports": {
    "#commands/*": "./dist/commands/*",
    "#components/*": "./dist/components/*",
    "#core/*": "./dist/core/*",
    "#events/*": "./dist/events/*",
    "#interfaces/*": "./dist/interfaces/*",
    "#lib/*": "./dist/lib/*",
    "#locales/*": "./dist/locales/*",
    "#db/*": "./dist/db/*"
  },
  "type": "module",
  "packageManager": "pnpm@9.3.0+sha512.ee7b93e0c2bd11409c6424f92b866f31d3ea1bef5fbe47d3c7500cdc3c9668833d2e55681ad66df5b640c61fa9dc25d546efa54d76d7f8bf54b13614ac293631"
